<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- <bean id="studbean" class="springcore.Student"> <property name="name" 
		value="shital"></property> </bean> -->

	<bean id="carbean" class="SpringDIwithSetterMethod.Car">
		<property name="carName" value="BMW"></property>
		<property name="carModel" value="x7"></property>
		<property name="eng" ref="enginebean"></property>
	</bean>

	<bean id="enginebean" class="SpringDIwithSetterMethod.Engine">
		<property name="engId" value="101"></property>
		<property name="engName" value="sdfg"></property>
	</bean>





</beans>  